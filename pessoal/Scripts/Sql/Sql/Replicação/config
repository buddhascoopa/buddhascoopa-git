      copy_rows => TRUE);
end;
/

/*---
adding master database 'HMLG' to master group 'G_SCOTT'.
---*/

begin
   dbms_repcat.add_master_database(
      gname => '"G_SCOTT"',
      master => 'HMLG.WORLD',
      use_existing_objects => TRUE,
      copy_rows => TRUE,
      propagation_mode => 'ASYNCHRONOUS');
end;
