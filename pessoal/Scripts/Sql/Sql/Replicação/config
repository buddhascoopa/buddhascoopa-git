      column_list => '"JOB"');
end;
/

/*---
generating replication support for table 'BONUS'.
---*/

begin 
    dbms_repcat.generate_replication_support(
      sname => '"SCOTT"',
      oname => '"BONUS"', 
      type => 'TABLE',
      min_communication => TRUE); 
end;
/

/*---
resuming replication on group 'G_SCOTT'.
---*/

begin 
   dbms_repcat.resume_master_activity(
      gname => '"G_SCOTT"'); 
end;
/

